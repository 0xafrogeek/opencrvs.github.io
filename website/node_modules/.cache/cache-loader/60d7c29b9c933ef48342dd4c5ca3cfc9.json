t { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\tid: 'implementations',\n\ttitle: 'Implementations'\n};\nexport const metadata = {\n  \"id\": \"community/implementations\",\n  \"title\": \"Implementations\",\n  \"description\": \"export const YouTubeVideo = ({ url }) => (\",\n  \"source\": \"@site/docs/community/Implementations_6a1c43c8ee414bcc8623db6cb698a1ff.mdx\",\n  \"permalink\": \"/opencrvs-core/docs/community/implementations\"\n};\n  /* @jsx mdx */\nimport useBaseUrl from \"@docusaurus/useBaseUrl\";\nexport const rightToc = [{\n  value: 'ðŸ‡§ðŸ‡© Bangladesh',\n  id: '-bangladesh',\n  children: []\n}, {\n  value: 'ðŸ‡¿ðŸ‡² Zambia',\n  id: '-zambia',\n  children: []\n}, {\n  value: '<strong>Interested?</strong>',\n  id: 'interested',\n  children: []\n}];\nexport const YouTubeVideo = ({\n  url\n}) => <div style={{\n  position: \"relative\",\n  paddingBottom: \"56.25%\",\n  height: 0\n}}>\n    <iframe width={560} height={315} src={url} frameBorder={0} allow=\"accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture\" allowFullScreen />\n  </div>;\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc,\nYouTubeVideo\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n\n\n    <h3 {...{\n      \"id\": \"-bangladesh\"\n    }}>{`ðŸ‡§ðŸ‡© Bangladesh`}</h3>\n    <p>{`Following a proof of concept, developed and demonstrated in 2019, the Government of Bangladesh, led by the Office of the Registrar General and the Cabinet Division, began a pilot of OpenCRVS in January 2020 in 2 sub-districts of the country.`}</p>\n    <YouTubeVideo url=\"https://www.youtube.com/embed/RmxVgE7WH40\" mdxType=\"YouTubeVideo\"></YouTubeVideo>\n    <h3 {...{\n      \"id\": \"-zambia\"\n    }}>{`ðŸ‡¿ðŸ‡² Zambia`}</h3>\n    <p>{`A proof of concept was developed in Zambia to demonstrate the product's viability as an ICT asset for the African continent. At the 5th Conference of Ministers responsible for CRVS in Africa, the Zambian version of OpenCRVS was showcased and received positive feedback as it responds directly to Resolution 19 of the Nouakchott Declaration which called for the \"development of common ICT assets to support effective CRVS systems across Africa\".`}</p>\n    <p>{`Martin Nyahoda, Deputy Registrar General in Zambia said: \"OpenCRVS is smart, it's a smart way of doing registration and I believe that if countries explore it, it's got the potential to improve service delivery and raise their registration coverage.\"`}</p>\n    <YouTubeVideo url=\"https://www.youtube.com/embed/912GKEhuHrE\" mdxType=\"YouTubeVideo\"></YouTubeVideo>\n    <h3 {...{\n      \"id\": \"interested\"\n    }}><strong parentName=\"h3\">{`Interested?`}</strong></h3>\n    <p>{`If you are interested in exploring how OpenCRVS can work for you, contact `}<a parentName=\"p\" {...{\n        \"href\": \"mailto:info@opencrvs.org\"\n      }}>{`info@opencrvs.org`}</a>{` for more information.`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}