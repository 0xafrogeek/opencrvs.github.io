tion\": \"Content management\",\n  \"source\": \"@site/docs/system_admin/Content_management_f7cca630a2624d2ab6c43e518f56e591.md\",\n  \"permalink\": \"/opencrvs-core/docs/system_admin/contentManagement\",\n  \"sidebar\": \"docs\",\n  \"previous\": {\n    \"title\": \"User & team management\",\n    \"permalink\": \"/opencrvs-core/docs/system_admin/userTeam\"\n  },\n  \"next\": {\n    \"title\": \"Communications management\",\n    \"permalink\": \"/opencrvs-core/docs/system_admin/commsManagement\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: 'Configuration',\n  id: 'configuration',\n  children: []\n}, {\n  value: 'User Stories',\n  id: 'user-stories',\n  children: [{\n    value: '<em>Coming Soon</em>',\n    id: 'coming-soon',\n    children: []\n  }]\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <h1 {...{\n      \"id\": \"content-management\"\n    }}>{`Content management`}</h1>\n    <p>{`This functionality allows you to easily manage the content (copy) of the application, including managing multiple languages.`}</p>\n    <h2 {...{\n      \"id\": \"configuration\"\n    }}>{`Configuration`}</h2>\n    <p>{`All product copy can be edited as per country requirements.`}</p>\n    <h2 {...{\n      \"id\": \"user-stories\"\n    }}>{`User Stories`}</h2>\n    <p>{`As a `}<strong parentName=\"p\">{`System Administrator,`}</strong>{` I want to be able to easily manage the content of the OpenCRVS application, so that I can focus on value add activities`}</p>\n    <h1 {...{\n      \"id\": \"functionality\"\n    }}>{`Functionality`}</h1>\n    <p>{`Currently, all content management is completed in the code by creating it in the country's resources package: `}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/opencrvs/opencrvs-zambia#languages---how-do-i-internationalise-or-update-the-opencrvs-client-text\"\n      }}>{`https://github.com/opencrvs/opencrvs-zambia#languages---how-do-i-internationalise-or-update-the-opencrvs-client-text`}</a></p>\n    <p>{`You can edit content by editing the following text file: `}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/opencrvs/opencrvs-zambia/blob/master/src/zmb/features/languages/generated/register.json\"\n      }}>{`https://github.com/opencrvs/opencrvs-zambia/blob/master/src/zmb/features/languages/generated/register.json`}</a></p>\n    <h3 {...{\n      \"id\": \"coming-soon\"\n    }}><em parentName=\"h3\">{`Coming Soon`}</em></h3>\n    <p>{`If OpenCRVS requires central management in the future, we will use an existing content management system (CMS) e.g. Contentful or Transifex`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}